/ {
	/* Pick serial console */
	chosen {
		stdout-path = "serial2:115200n8";
	};

	memory@40000000 {
		reg = <0x60000000 0x1f000000>;
	};

	vm-memory@40000000 {
		reg = <0x40000000 0x20000000>;
	};

};
