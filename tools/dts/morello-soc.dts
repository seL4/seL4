/dts-v1/;

/ {
	compatible = "arm,morello";
	interrupt-parent = <0x01>;
	#address-cells = <0x02>;
	#size-cells = <0x02>;
	model = "Arm Morello System Development Platform";

	aliases {
		serial0 = "/serial@2a400000";
