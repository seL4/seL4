	timer@1c0d0000{
		compatible = "arm,sp804", "arm,primecell";
		reg = <0x0 0x1c0d0000 0x0 0x1000>;
		interrupts = <0x00 0x8f 0x04>;
		clocks = <0x14 0x0 0x14 0x1 0x13>;
		clock-names = "timclken1", "timclken2", "apb_pclk";
	};

