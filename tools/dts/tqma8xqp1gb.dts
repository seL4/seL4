			clock-frequency = <0x7de2900>;
			clock-output-names = "cm40_ipg_clk";
			phandle = <0x13>;
		};

		i2c@37230000 {
			compatible = "fsl,imx8qxp-lpi2c", "fsl,imx7ulp-lpi2c";
			reg = <0x37230000 0x1000>;
			interrupts = <0x9 0x4>;
			interrupt-parent = <0x11>;
			clocks = <0x12 0x0 0x12 0x1>;
