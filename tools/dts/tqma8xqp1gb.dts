			};

			mba8xxhoggrp {
				fsl,pins = <0x75 0x4 0x48 0x17 0x4 0x48>;
				phandle = <0x8>;
			};

			lpuart1grp {
				fsl,pins = <0x4e 0x0 0x6000020 0x4d 0x0 0x6000020>;
				phandle = <0x60>;
			};

			lpuart3grp {
				fsl,pins = <0x6d 0x2 0x6000020 0x6e 0x2 0x6000020>;
				phandle = <0x63>;
			};

			pca9538grp {
				fsl,pins = <0x9b 0x4 0x20>;
