			};

			regusbotg1vbusgrp {
				fsl,pins = <0x4 0x4 0x21>;
				phandle = <0xd4>;
			};

			spi1grp {
				fsl,pins = <0x54 0x2 0x6000041 0x56 0x2 0x6000041 0x53 0x2 0x6000041 0x52 0x2 0x6000041 0x55 0x2 0x6000041>;
