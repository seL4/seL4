			clocks = < 0x02 0x39 0x02 0x96 0x02 0x9d >;
			clock-names = "ahb\0tcon-ch0\0tcon-ch1";
			clock-output-names = "tcon1-pixel-clock";
			dmas = < 0x0b 0x01 0x0f >;

			ports {
				#address-cells = < 0x01 >;
				#size-cells = < 0x00 >;

				port@0 {
					#address-cells = < 0x01 >;
