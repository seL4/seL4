						phandle = < 0x3d >;
					};

					endpoint@1 {
						reg = < 0x01 >;
						remote-endpoint = < 0x0e >;
						phandle = < 0x39 >;
					};
				};

				port@1 {
					#address-cells = < 0x01 >;
